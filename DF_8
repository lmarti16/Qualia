# Solo Distrito Federal 8
SeccionesR_DF_8 <- filter(Resultados, DISTRITO == 8)
Secciones_DF_8 <- filter(Secciones_OAX, DISTRITO == 8)
Manzanas_DF_8 <- filter(Manzanas_OAX, DISTRITO_F == 8)
ESEM_Oax <- st_transform(ESEM_Oax, st_crs(Secciones_DF_8))
ESEM_DF_8 <- st_intersection(ESEM_DF_8, Secciones_DF_8)

# Definir la paleta de colores
colores <- c("Muy Alto" = "blue",
             "Alto" = "#00CC00",
             "Medio alto" = "#99FF99",
             "Medio" = "#FFFF99",
             "Medio bajo" = "#FFCC99",
             "Bajo" = "#FF3300",
             "Muy Bajo" = "brown",
             "No determinado" = "#808080")

# Seleccionar columnas y cambiar nombres
SeccionesR_DF_8 <- SeccionesR_DF_8 %>%
    select(-"DISTRITO_L", -"DISTRITO", -"MUNICIPIO", -"ENTIDAD", -"NOMBRE_ENTIDAD")

# Convertir las columnas, excluyendo las que deben permanecer como texto
SeccionesR_DF_8 <- SeccionesR_DF_8 %>%
    mutate(across(-c(Ganador_Partido, Ganador_coalición, Ganador_original,
                     Gene_prepo, NOMBRE_ESTADO,
                     GANADOR_21, Cambio_Ganador), as.numeric, errors = "coerce"))
# Crear quintiles
quintilesF <- quantile(SeccionesR_DF_8$FAM, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesFP <- quantile(SeccionesR_DF_8$FAM_PRI, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesP <- quantile(SeccionesR_DF_8$PAN, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesJ <- quantile(SeccionesR_DF_8$JHH, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesPF <- quantile(SeccionesR_DF_8$diferenciaPF, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesTF <- quantile(SeccionesR_DF_8$diferenciaTF, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesPFP <- quantile(SeccionesR_DF_8$diferenciaPFP, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesTFP <- quantile(SeccionesR_DF_8$diferenciaTFP, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesDif <- quantile(SeccionesR_DF_8$diferencia_2122, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesDifp <- quantile(SeccionesR_DF_8$diferencia_2122por, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)

# Crear quintiles de generación
quintilesz <- quantile(SeccionesR_DF_8$GEN_Z_TOTAL_PORCENTAJE, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesMi <- quantile(SeccionesR_DF_8$MILLENNIALS_TOTAL_PORCENTAJE, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesgx <- quantile(SeccionesR_DF_8$GEN_X_TOTAL_PORCENTAJE, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesbb <- quantile(SeccionesR_DF_8$BOOMERS_TOTAL_PORCENTAJE, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesSi <- quantile(SeccionesR_DF_8$SILENT_TOTAL_PORCENTAJE, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)

# Agrega columna con quintiles
SeccionesR_DF_8$quintilF <- cut(SeccionesR_DF_8$FAM, breaks = quintilesF, include.lowest = TRUE)
SeccionesR_DF_8$quintilFP <- cut(SeccionesR_DF_8$FAM_PRI, breaks = quintilesFP, include.lowest = TRUE)
SeccionesR_DF_8$quintilP <- cut(SeccionesR_DF_8$PAN, breaks = quintilesP, include.lowest = TRUE)
SeccionesR_DF_8$quintilJ <- cut(SeccionesR_DF_8$JHH, breaks = quintilesJ, include.lowest = TRUE)
SeccionesR_DF_8$quintilPF <- cut(SeccionesR_DF_8$diferenciaPF, breaks = quintilesPF, include.lowest = TRUE)
SeccionesR_DF_8$quintilTF <- cut(SeccionesR_DF_8$diferenciaTF, breaks = quintilesTF, include.lowest = TRUE)
SeccionesR_DF_8$quintilPFP <- cut(SeccionesR_DF_8$diferenciaPFP, breaks = quintilesPFP, include.lowest = TRUE)
SeccionesR_DF_8$quintilTFP <- cut(SeccionesR_DF_8$diferenciaTFP, breaks = quintilesTFP, include.lowest = TRUE)
SeccionesR_DF_8$quintilDif <- cut(SeccionesR_DF_8$diferencia_2122, breaks = quintilesDif, include.lowest = TRUE)
SeccionesR_DF_8$quintilDifP <- cut(SeccionesR_DF_8$diferencia_2122por, breaks = quintilesDifp, include.lowest = TRUE)

# Agrega columna con quintiles de generacion
SeccionesR_DF_8$quintilZ <- cut(SeccionesR_DF_8$GEN_Z_TOTAL_PORCENTAJE, breaks = quintilesz, include.lowest = TRUE)
SeccionesR_DF_8$quintilMI <- cut(SeccionesR_DF_8$MILLENNIALS_TOTAL_PORCENTAJE, breaks = quintilesMi, include.lowest = TRUE)
SeccionesR_DF_8$quintilGX <- cut(SeccionesR_DF_8$GEN_X_TOTAL_PORCENTAJE, breaks = quintilesgx, include.lowest = TRUE)
SeccionesR_DF_8$quintilBB <- cut(SeccionesR_DF_8$BOOMERS_TOTAL_PORCENTAJE, breaks = quintilesbb, include.lowest = TRUE)
SeccionesR_DF_8$quintilSI <- cut(SeccionesR_DF_8$SILENT_TOTAL_PORCENTAJE, breaks = quintilesSi, include.lowest = TRUE)

# Crear la columna C_FAM
SeccionesR_DF_8 <- SeccionesR_DF_8 %>%
    mutate(C_FAM = rowSums(select(SeccionesR_DF_8, "CANDIDATURA COMÚN PRI-PRD"), na.rm = TRUE))

# Crear la columna C_JHH
cols_C_JHH <- c("COALICIÓN PT-PVEM-PUP-MORENA", "PT-PVEM-PUP", "PT-PVEM-MORENA",
                "PT-PUP-MORENA", "PVEM-PUP-MORENA", "PT-PVEM", "PT-PUP",
                "PT-MORENA", "PVEM-PUP", "PVEM-MORENA", "PUP-MORENA","PUP")

SeccionesR_DF_8 <- SeccionesR_DF_8 %>%
    mutate(C_JHH = rowSums(select(SeccionesR_DF_8, all_of(cols_C_JHH)), na.rm = TRUE))

# Crear la columna C_FAM_21
cols_C_FAM_21 <- c("PAN-PRI-PRD_21", "PAN-PRI_21", "PAN-PRD_21", "PRI-PRD_21")

SeccionesR_DF_8 <- SeccionesR_DF_8 %>%
    mutate(C_FAM_21 = rowSums(select(SeccionesR_DF_8, all_of(cols_C_FAM_21)), na.rm = TRUE))

# Crear la columna C_JHH_21
cols_C_JHH_21 <- c("PVEM-PT-MORENA_21", "PVEM-PT_21", "PVEM-MORENA_21", "PT-MORENA_21")

SeccionesR_DF_8 <- SeccionesR_DF_8 %>%
    mutate(C_JHH_21 = rowSums(select(SeccionesR_DF_8, all_of(cols_C_JHH_21)), na.rm = TRUE))

# Seleccionar las columnas requeridas
columnas_seleccionadas <- c("SECCION", "PAN", "PRI", "PRD", "PT", "PVEM", "MC", "MORENA",
                            "PAN_21", "PRI_21", "MC_21", "PRD_21", "PVEM_21", "PT_21", "MORENA_21",
                            "C_FAM", "C_JHH", "C_FAM_21", "C_JHH_21","Ganador_coalición","GANADOR_21")

# Crear el nuevo DataFrame Cambios_DF_8
Cambios_DF_8 <- SeccionesR_DF_8[columnas_seleccionadas]

# Obtener las columnas a modificar
columnas_a_modificar <- c("PAN", "PRI", "PRD", "PT", "PVEM", "MORENA","MC","C_FAM","C_JHH")

# Renombrar las columnas
colnames(Cambios_DF_8)[colnames(Cambios_DF_8) %in% columnas_a_modificar] <- paste0(colnames(Cambios_DF_8)[colnames(Cambios_DF_8) %in% columnas_a_modificar], "_22")

# Cambiar el nombre de la columna Ganador_coalición por GANADOR_22
Cambios_DF_8 <- Cambios_DF_8 %>% 
    rename(GANADOR_22 = Ganador_coalición)

# CREAR el mapa manzanas
mapaMDF_8 <- left_join(Manzanas_DF_8, SeccionesR_DF_8, by = c("SECCION"))
# CREAR el mapa secciones
mapaSDF_8 <- left_join(Secciones_DF_8, SeccionesR_DF_8, by = c("SECCION"))

# Mapa de Resultados22
mapa_resultados22 <- ggplot() +
    annotation_map_tile(type = "osm", zoom = 15) +
    geom_sf(data = mapaMDF_8, aes(fill = Ganador_coalición), alpha = .7, color = NA) +
    geom_sf(data = Secciones_DF_8, aes(fill = NULL), color = "black", size = 0.1, alpha = 0) +
    geom_sf_label(data = Secciones_DF_8, aes(label = SECCION), size = 1, fill = "white", color = "black", alpha = 0 , label.size = NA) +
    scale_fill_manual(values = c("JHH" = "brown", "FAM" = "darkblue","MC" = "orange", "EMPATE" ="grey" ), name = "Resultado electoral\n de la Manzana", labels = c("Frente Amplió Por México", "Juntos Haremos Historia")) +
    labs(title = "¿Qué coalición Ganaría en Distrito Federal 8 en 2022?",
         subtitle = "Por manzanas habitadas",
         caption = paste0("Elaborado por Qualia: Asesoría Especializada\n(Fuente: INEGI e INE/)\n",
                          format(Sys.Date(), "%d/%m/%Y"))) +
    theme_economist(base_family="ITC Officina Sans") +
    guides(fill = guide_legend(ncol = 2)) +
    theme(legend.position = "bottom",
          legend.background = element_rect(fill = "#014d64"),
          legend.text = element_text(size = 10, color = "white"),
          legend.title = element_text(size = 12, color = "white", face = "bold"),
          axis.text = element_blank(),
          axis.title = element_blank(),
          axis.line = element_blank(),
          panel.background = element_blank(),
          panel.grid.major = element_blank(),
          panel.grid.minor = element_blank(),
          plot.title = element_text(hjust = 0.5,size = 24, color = "#014d64", face = "bold"),
          plot.caption = element_text(hjust = 0.5,size = 18, color = "#014d64", face = "bold"),
          plot.subtitle = element_text(hjust = 0.5,size = 20, color = "#014d64", face = "bold"))
# Guardar el primer mapa como PDF
ggsave("D:\\52556\\Documents\\Nassar-oax\\DF_8\\mapa_resultados22.pdf", 
       plot = mapa_resultados22, 
       width = 8.5, height = 11, units = "in", device = "pdf")

