#Mapa Diferencia
Mapa_Diferencia <- ggplot() +
    annotation_map_tile(type = "osm", zoom = 15) +
    geom_sf(data = mapaMDF_8, aes(fill = quintilPF), alpha = .7, color = NA) +
    geom_sf(data = Secciones_DF_8, aes(fill = NULL), color = "black", size = 0.1, alpha = 0) +
    geom_sf_label(data = Secciones_DF_8, aes(label = SECCION), size = 1, fill = "white", color = "black", alpha = 0 , label.size = NA) +
