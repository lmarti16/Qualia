    guides(fill = guide_legend(nrow = 3))  # Dos filas en la caja de leyendas
# Guardar el primer mapa como PDF
ggsave("D:\\52556\\Documents\\Nassar-oax\\DF_8\\Mapa_Silent.pdf", 
       plot = Mapa_Silent, 
       width = 8.5, height = 11, units = "in", device = "pdf")



