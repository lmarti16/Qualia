# Leer archivo SHP usando read_sf()
Manzanas_MICHOACAN <- read_sf("D:\\Descargas\\SHAPEFILES_MEX\\16 MICHOACAN\\MANZANA.shp")
sec_mex <- read_sf("D:\\Descargas\\secciones_2024.gpkg\\secciones_2024.gpkg")
DistritoF_MICHOACAN <- read_sf("D:\\Descargas\\SHAPEFILES_MEX\\16 MICHOACAN\\DISTRITO_FEDERAL.shp")
DistritoL_MICHOACAN <- read_sf("D:\\Descargas\\SHAPEFILES_MEX\\16 MICHOACAN\\DISTRITO_LOCAL.shp")
Resultados21 <- read_csv("D:\\52556\\Documents\\Secretaria_GralPRI\\Resultados_resec.csv")
Resultados18 <- read_csv("D:\\52556\\Documents\\twitter\\Resultados_18(resec).csv")
Lista_Nominal_Selected <- read_sf("D:\\52556\\Documents\\Secretaria_GralPRI\\2024_LN.csv")
Municipios_MICHOACAN <- read_sf("D:\\Descargas\\SHAPEFILES_MEX\\16 MICHOACAN\\MUNICIPIO.shp")
Secciones_Switcher <- read_sf("D:\\52556\\Documents\\Secretaria_GralPRI\\secciones_switcher.csv")
Colonias_MX <- read_sf("D:\\Descargas\\imc2020_shp\\colonias_imc2020.shp")

Secciones_Switcher$SECCION <- as.numeric(Secciones_Switcher$SECCION)
Secciones_Switcher$ENTIDAD <- as.numeric(Secciones_Switcher$ENTIDAD)

Lista_Nominal_Selected <- Lista_Nominal_Selected %>%
  mutate(across(-Gene_prepo, as.numeric))

sec_mex <- sec_mex %>%
    select(-PADRON_HOMBRES, -PADRON_MUJERES,  -PADRON_ELECTORAL, -LISTA_HOMBRES, -LISTA_MUJERES, -LISTA_NOMINAL)

# Reemplaza los espacios por saltos de línea en la columna NOMBRE
Municipios_MICHOACAN$NOMBRE <- str_replace_all(Municipios_MICHOACAN$NOMBRE, " ", "\n")

# Suponiendo que tu dataframe se llama 'Municipios_MICHOACAN'
Municipios_MICHOACAN <- Municipios_MICHOACAN %>%
  mutate(NOMBRE = paste0(tools::toTitleCase(tolower(NOMBRE))))

# Asumiendo que tu tibble se llama Secciones_Switcher
Secciones_Switcher <- Secciones_Switcher %>%
  mutate(
    Switcher = if_else(Ganador_21 != Ganador_18, "SI", "NO"),
    Switcher_coalición = case_when(
      Ganador_18 %in% c("MORENA", "PT", "ES") & Ganador_21 %in% c("PRI", "PAN", "PRD") ~ "Cambió a FAM",
      Ganador_18 %in% c("PRI", "PAN", "PRD") & Ganador_21 %in% c("MORENA", "PT", "ES", "PVEM") ~ "Cambio a SHH",
      TRUE ~ NA_character_ # Para cualquier otra condición no especificada
    )
  )

Resultados21 <- Resultados21 %>%
    select(-CIRCUNSCRIPCION, -NOMBRE_ESTADO, -ID_DISTRITO_FEDERAL, 
           -CABECERA_DISTRITAL_FEDERAL, -ID_MUNICIPIO, -MUNICIPIO, -CASILLAS)

# Convertir todas las columnas a numérico
Resultados21 <- Resultados21 %>%
    mutate(across(everything(), as.numeric))

# Renombrar columnas excepto las especificadas
Resultados21 <- Resultados21 %>%
    rename_with(~ paste0(., "_21"), -c(FAM_21, SHH_21, ENTIDAD, SECCION))

Resultados18 <- Resultados18 %>%
    select(-CIRCUNSCRIPCION, -NOMBRE_ESTADO, -ID_DISTRITO, 
           -CABECERA_DISTRITAL, -ID_MUNICIPIO, -MUNICIPIO, -CASILLAS)

# Convertir todas las columnas a numérico
Resultados18 <- Resultados18 %>%
    mutate(across(everything(), as.numeric))

# Renombrar columnas excepto las especificadas
Resultados18 <- Resultados18 %>%
    rename_with(~ paste0(., "_18"), -c(FAM_18, SHH_18, ENTIDAD, SECCION))

Resultados_MICHOACAN <- filter(Resultados21, ENTIDAD == 16)
Resultados_MICHOACAN <- left_join(Resultados_MICHOACAN, Resultados18, by = c("SECCION", "ENTIDAD"))
Secciones_MICHOACAN <- filter(sec_mex, ENTIDAD == 16)


Secciones_MICHOACAN_DL17<- filter(Secciones_MICHOACAN, DISTRITO_LOCAL== 17)
Manzanas_MICHOACAN_DL17 <- filter(Manzanas_MICHOACAN, DISTRITO_L == 17)
DistritoL_MICHOACAN_DL17 <- filter(DistritoL_MICHOACAN, DISTRITO_L == 17)

# Obtener los valores únicos de MUNICIPIO y DISTRITO_L de Secciones_MICHOACAN_DL17 
Municipios_DL17 <- unique(Secciones_MICHOACAN_DL17$MUNICIPIO)
Valores_DF_DL17 <- unique(Secciones_MICHOACAN_DL17$DISTRITO_FEDERAL)

# Filtrar DistritoF_MICHOACAN, DistritoL_MICHOACAN y circunscupcion
Municipios_MICHOACAN_DL17 <- Municipios_MICHOACAN[Municipios_MICHOACAN$MUNICIPIO %in% Municipios_DL17, ]
DistritoF_MICHOACAN_DL17 <- DistritoF_MICHOACAN[DistritoF_MICHOACAN$DISTRITO %in% Valores_DF_DL17, ]

#Define Colonias
Colonias_MICHOACAN <- filter(Colonias_MX, CVE_ENT == "16")
Colonias_MICHOACAN <- st_transform(Colonias_MICHOACAN, st_crs(DistritoL_MICHOACAN_DL17 ))
Colonias_MICHOACAN_DL17 <- st_intersection(DistritoL_MICHOACAN_DL17 , Colonias_MICHOACAN)

# Calculamos el área de cada geometría resultante
Colonias_MICHOACAN_DL17$area <- st_area(Colonias_MICHOACAN_DL17)

# Definimos un umbral mínimo de área como un objeto de tipo 'units'
umbral_area <- set_units(1000, m^2) # Ajusta este valor según tus necesidades

# Filtramos para excluir geometrías con área menor al umbral
Colonias_MICHOACAN_DL17 <- Colonias_MICHOACAN_DL17[Colonias_MICHOACAN_DL17$area > umbral_area, ]

# Unir los dataframes por SECCION y ENTIDAD
Resultados_DL17 <- left_join(Secciones_MICHOACAN_DL17, Resultados_MICHOACAN, by = c("SECCION", "ENTIDAD"))
Resultados_DL17 <- st_drop_geometry(Resultados_DL17)
Resultados_DL17 <- left_join(Resultados_DL17, Secciones_Switcher, by = c("SECCION", "ENTIDAD"))
Resultados_DL17 <- left_join(Resultados_DL17, Lista_Nominal_Selected, by = c("SECCION", "ENTIDAD"))

Resultados_DL17 <- Resultados_DL17 %>% 
    mutate( porFAM_21 = (FAM_21 /TOTAL_VOTOS_21)*100) %>% 
    mutate( porPRI_21 = (PRI_21 /TOTAL_VOTOS_21)*100) %>% 
    mutate( porPRD_21 = (PRD_21 /TOTAL_VOTOS_21)*100) %>% 
    mutate( porPAN_21 = (PAN_21 /TOTAL_VOTOS_21)*100) %>% 
    mutate( porMORENA_21 = (SHH_21 /TOTAL_VOTOS_21)*100) %>% 
    mutate( porSHH_21 = (SHH_21 /TOTAL_VOTOS_21)*100) %>% 
    mutate( porMC_18 = (MC_18 /TOTAL_VOTOS_18)*100) %>% 
    mutate( participacion = (TOTAL_VOTOS_21 /LISTA_NOMINAL_21)*100) %>% 
    mutate( diferencia =  porFAM_21 - porSHH_21) %>%
    mutate( diferenciaPRD = porFAM_21 - porPRD_21) %>%
    mutate( diferenciaPRI = porFAM_21 - porPRI_21) %>%
    mutate( diferenciaPAN = porFAM_21 - porPAN_21)  %>%
    mutate( VsinPAN = FAM_21 - PAN_21) %>%
    mutate( VsinPRI = FAM_21 - PRI_21) %>%
    mutate( VsinPRD = FAM_21 - PRD_21) %>%
    mutate( Vdif = FAM_21 - SHH_21) %>%
    mutate( Tdif = FAM_21 - FAM_18) %>%
    mutate( TPAN = PAN_21 - PAN_18) %>%
    mutate( Otro_P = PES_21 + RSP_21+ FXM_21) 

# Crear quintiles
quintilesPRD <- quantile(Resultados_DL17$diferenciaPRD, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesM <- quantile(Resultados_DL17$porSHH_21, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesD <- quantile(Resultados_DL17$diferencia, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesparti <- quantile(Resultados_DL17$participacion, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesLN <- quantile(Resultados_DL17$LISTA_NOMINAL_21, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesPAN <- quantile(Resultados_DL17$diferenciaPAN, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesPRI <- quantile(Resultados_DL17$diferenciaPRI, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesTdif <- quantile(Resultados_DL17$Tdif, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesTPAN <- quantile(Resultados_DL17$TPAN, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesOtro <- quantile(Resultados_DL17$Otro_P, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesSHH <- quantile(Resultados_DL17$SHH_21, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesMC <- quantile(Resultados_DL17$MC_21, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesPVEM <- quantile(Resultados_DL17$PVEM_21, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)
quintilesPT<- quantile(Resultados_DL17$PT_21, probs = c(0, 0.2, 0.4, 0.6, 0.8, 1), na.rm = TRUE)


# Definir etiquetas personalizadas
labels_custom <- function(breaks) {
  n <- length(breaks) - 1
  labels <- character(n)
  for (i in 1:n) {
    labels[i] <- ifelse(i < n, paste0("(", ceiling(breaks[i]), ", ", floor(breaks[i + 1]), "]"), paste0("[", ceiling(breaks[i]), ", ", ceiling(breaks[i + 1]), "]"))
  }
  return(labels)
}

# Agrega columna con quintiles y etiquetas personalizadas
Resultados_DL17$quintilPRD <- cut(Resultados_DL17$diferenciaPRD, breaks = quintilesPRD, include.lowest = TRUE, labels = labels_custom(quintilesPRD))
Resultados_DL17$quintilM <- cut(Resultados_DL17$porSHH_21, breaks = quintilesM, include.lowest = TRUE, labels = labels_custom(quintilesM))
Resultados_DL17$quintilD <- cut(Resultados_DL17$diferencia, breaks = quintilesD, include.lowest = TRUE, labels = labels_custom(quintilesD))
Resultados_DL17$quintilparti <- cut(Resultados_DL17$participacion, breaks = quintilesparti, include.lowest = TRUE, labels = labels_custom(quintilesparti))
Resultados_DL17$quintilLN <- cut(Resultados_DL17$LISTA_NOMINAL_21, breaks = quintilesLN, include.lowest = TRUE, labels = labels_custom(quintilesLN))
Resultados_DL17$quintilPRI <- cut(Resultados_DL17$diferenciaPRI, breaks = quintilesPRI, include.lowest = TRUE, labels = labels_custom(quintilesPRI))
Resultados_DL17$quintilPAN <- cut(Resultados_DL17$diferenciaPAN, breaks = quintilesPAN, include.lowest = TRUE, labels = labels_custom(quintilesPAN))
Resultados_DL17$quintilTdif  <- cut(Resultados_DL17$Tdif , breaks = quintilesTdif , include.lowest = TRUE, labels = labels_custom(quintilesTdif))
Resultados_DL17$quintilTPAN  <- cut(Resultados_DL17$TPAN , breaks = quintilesTPAN , include.lowest = TRUE, labels = labels_custom(quintilesTPAN))
Resultados_DL17$quintilOtro  <- cut(Resultados_DL17$Otro_P , breaks = quintilesOtro , include.lowest = TRUE, labels = labels_custom(quintilesOtro))
Resultados_DL17$quintilSHH  <- cut(Resultados_DL17$SHH_21 , breaks = quintilesSHH , include.lowest = TRUE, labels = labels_custom(quintilesSHH))
Resultados_DL17$quintilMC <- cut(Resultados_DL17$MC_21 , breaks = quintilesMC , include.lowest = TRUE, labels = labels_custom(quintilesMC))
Resultados_DL17$quintilPVEM <- cut(Resultados_DL17$PVEM_21 , breaks = quintilesPVEM , include.lowest = TRUE, labels = labels_custom(quintilesPVEM))
Resultados_DL17$quintilPT <- cut(Resultados_DL17$PT_21 , breaks = quintilesPT , include.lowest = TRUE, labels = labels_custom(quintilesPT))

Resultados_DL17 <- Resultados_DL17 %>%
    rowwise() %>%
    mutate(GanadorC_21 = {
        votos <- c(FAM = FAM_21, SHH = SHH_21)
        votos[is.na(votos)] <- 0 # Convertir NA a 0
        if (all(votos == 0)) {
            NA_character_ # Retornar NA si todos los votos son 0
        } else {
            names(which.max(votos))
        }
    })

Resultados_DL17 <- Resultados_DL17 %>%
    rowwise() %>%
    mutate(Ganador_21 = {
        votos <- c(PAN = PAN_21, PRI = PRI_21, PRD = PRD_21, PVEM = PVEM_21, PT = PT_21, MC = MC_21, RSP = RSP_21,ES = PES_21,MORENA = MORENA_21,FXM = FXM_21)
        votos[is.na(votos)] <- 0 # Convertir NA a 0
        if (all(votos == 0)) {
            NA_character_ # Retornar NA si todos los votos son 0
        } else {
            names(which.max(votos))
        }
    })

Resultados_DL17 <- Resultados_DL17 %>%
    rowwise() %>%
    mutate(Ganador_18 = {
        votos <- c(PAN = PAN_21, PRI = PRI_21, PRD = PRD_21, PVEM = PVEM_21, PT = PT_21, MC = MC_21, ES = ES_18,MORENA = MORENA_18)
        votos[is.na(votos)] <- 0 # Convertir NA a 0
        if (all(votos == 0)) {
            NA_character_ # Retornar NA si todos los votos son 0
        } else {
            names(which.max(votos))
        }
    })


Resultados_DL17 <- Resultados_DL17 %>%
    rowwise() %>%
    mutate(Ganador_otro = {
        votos <- c(RSP = RSP_21,ES = PES_21,FXM = FXM_21)
        votos[is.na(votos)] <- 0 # Convertir NA a 0
        if (all(votos == 0)) {
            NA_character_ # Retornar NA si todos los votos son 0
        } else {
            names(which.max(votos))
        }
    })

Resultados_DL17 <- Resultados_DL17 %>%
    rowwise() %>%
    mutate(Ganador_FAM= {
        votos <- c(PAN = PAN_21, PRI = PRI_21, PRD = PRD_21)
        votos[is.na(votos)] <- 0 # Convertir NA a 0
        if (all(votos == 0)) {
            NA_character_ # Retornar NA si todos los votos son 0
        } else {
            names(which.max(votos))
        }
    })

# Asumiendo que tu tibble se llama Secciones_Switcher
Resultados_DL17 <- Resultados_DL17 %>%
  mutate(
    Switcher = if_else(Ganador_21 != Ganador_18, "SI", "NO"),
    Switcher_coalición = case_when(
      Ganador_18 %in% c("MORENA", "PT", "ES") & Ganador_21 %in% c("PRI", "PAN", "PRD") ~ "Cambió a FAM",
      Ganador_18 %in% c("PRI", "PAN", "PRD") & Ganador_21 %in% c("MORENA") ~ "Cambio a SHH",
      TRUE ~ NA_character_ # Para cualquier otra condición no especificada
    )
  )


Resultados_DL17 <- Resultados_DL17 %>%
    mutate(Sugerencia = case_when(
        # Prioritaria: La más alta participación y lista nominal, y en el quintil 3 de diferencia
        quintilLN == labels_custom(quintilesLN)[5] & quintilD == labels_custom(quintilesD)[5] & quintilparti == labels_custom(quintilesparti)[3] ~ "Sugerencia prioritaria",
        
        # Secundaria: Primera o segunda más alta participación, también primera o segunda más alta lista nominal, y en el quintil 3 de diferencia
        (quintilLN %in% c(labels_custom(quintilesLN)[4], labels_custom(quintilesLN)[5]) &
             quintilparti %in% c(labels_custom(quintilesparti)[4], labels_custom(quintilesparti)[5]) &
             quintilD == labels_custom(quintilesD)[3]) ~ "Sugerencia secundaria",
        
        # Terciaria: En el quintil 3 de diferencia, o todas las secciones en los quintiles 4-5 de LN o de participación
        quintilD == labels_custom(quintilesD)[3] |
            quintilLN %in% c(labels_custom(quintilesLN)[5]) |
            quintilparti %in% c(labels_custom(quintilesparti)[5]) ~ "Sugerencia terciaria",
        
        # Caso predeterminado
        TRUE ~ "Sección no sugerida"
    ))

Resultados_DL17 <- Resultados_DL17 %>%
    select(-NOMBRE_ENTIDAD, -MUNICIPIO, -NOMBRE_MUNICIPIO, 
           -DISTRITO_FEDERAL, -NOMBRE_DISTRITO_FEDERAL, -DISTRITO_LOCAL)

mapa_DL17 <- left_join(Secciones_MICHOACAN_DL17 , Resultados_DL17, by = c("SECCION", "ENTIDAD"))

# Corrección en la definición de rangosQuintiles para mantener la consistencia
rangosQuintiles <- list(
  F = quintilesPRD,
  M = quintilesM,
  D = quintilesD,
  P = quintilesparti, # Cambiado a quintilesparti para mantener la consistencia
  LN = quintilesLN
)

# Corrección en la creación de las columnas CompetitividadD y CompetitividadPRD
mapa_DL17 <- mapa_DL17 %>%
  mutate(CompetitividadD = case_when(
    quintilD == labels_custom(quintilesD)[1] ~ "Muy poca Competitividad",
    quintilD == labels_custom(quintilesD)[2] ~ "Poca Competitividad",
    quintilD == labels_custom(quintilesD)[3] ~ "Competitividad media",
    quintilD == labels_custom(quintilesD)[4] ~ "Alta Competitividad",
    quintilD == labels_custom(quintilesD)[5] ~ "Muy alta Competitividad"
  )) %>%
  mutate(CompetitividadPRD = case_when(
    quintilPRD == labels_custom(quintilesPRD)[5] ~ "Muy Poco Impacto",
    quintilPRD == labels_custom(quintilesPRD)[4] ~ "Poco Impacto",
    quintilPRD == labels_custom(quintilesPRD)[3] ~ "Impacto Medio",
    quintilPRD == labels_custom(quintilesPRD)[2] ~ "Alto Impacto",
    quintilPRD == labels_custom(quintilesPRD)[1] ~ "Muy Alto Impacto"
  )) %>%
  mutate(Tamaños_lista = case_when(
    quintilLN == labels_custom(quintilesLN)[1] ~ "La más baja",
    quintilLN == labels_custom(quintilesLN)[2] ~ "Baja",
    quintilLN == labels_custom(quintilesLN)[3] ~ "Media",
    quintilLN == labels_custom(quintilesLN)[4] ~ "Alta",
    quintilLN == labels_custom(quintilesLN)[5] ~ "La más alta"
  )) %>%
  mutate(Porcen_parti = case_when(
    quintilparti == labels_custom(quintilesparti)[1] ~ "Muy poca participación",
    quintilparti == labels_custom(quintilesparti)[2] ~ "Poca participación",
    quintilparti == labels_custom(quintilesparti)[3] ~ "Participación media",
    quintilparti == labels_custom(quintilesparti)[4] ~ "Alta participación",
    quintilparti == labels_custom(quintilesparti)[5] ~ "Muy alta participación"
  )) %>%
  mutate(CompetitividadPRI = case_when(
    quintilPRI == labels_custom(quintilesPRI)[5] ~ "Muy Poco Impacto",
    quintilPRI == labels_custom(quintilesPRI)[4] ~ "Poco Impacto",
    quintilPRI == labels_custom(quintilesPRI)[3] ~ "Impacto Medio",
    quintilPRI == labels_custom(quintilesPRI)[2] ~ "Alto Impacto",
    quintilPRI == labels_custom(quintilesPRI)[1] ~ "Muy Alto Impacto"
  )) %>%
  mutate(CompetitividadPAN = case_when(
    quintilPAN == labels_custom(quintilesPAN)[5] ~ "Muy Poco Impacto",
    quintilPAN == labels_custom(quintilesPAN)[4] ~ "Poco Impacto",
    quintilPAN == labels_custom(quintilesPAN)[3] ~ "Impacto Medio",
    quintilPAN == labels_custom(quintilesPAN)[2] ~ "Alto Impacto",
    quintilPAN == labels_custom(quintilesPAN)[1] ~ "Muy Alto Impacto"
  )) %>%
  mutate(PerdidaFAM = case_when(
    quintilTdif == labels_custom(quintilesTdif)[5] ~ "Ganó o Perdió pocos Votos",
    quintilTdif == labels_custom(quintilesTdif)[4] ~ "Cuarta Mayor perdida de votos",
    quintilTdif == labels_custom(quintilesTdif)[3] ~ "Tercera Mayor perdida de votos",
    quintilTdif == labels_custom(quintilesTdif)[2] ~ "Segunda Mayor perdida de votos",
    quintilTdif == labels_custom(quintilesTdif)[1] ~ "Mayor perdida de votos"
  )) %>%
  mutate(PerdidaPAN = case_when(
    quintilTPAN == labels_custom(quintilesTPAN)[5] ~ "Ganó o Perdió pocos Votos",
    quintilTPAN == labels_custom(quintilesTPAN)[4] ~ "Cuarta Mayor perdida de votos",
    quintilTPAN == labels_custom(quintilesTPAN)[3] ~ "Tercera Mayor perdida de votos",
    quintilTPAN == labels_custom(quintilesTPAN)[2] ~ "Segunda Mayor perdida de votos",
    quintilTPAN == labels_custom(quintilesTPAN)[1] ~ "Mayor perdida de votos"
  )) %>%
  mutate(CompetitividadOtro = case_when(
    quintilOtro == labels_custom(quintilesOtro)[1] ~ "La más baja",
    quintilOtro == labels_custom(quintilesOtro)[2] ~ "Baja",
    quintilOtro == labels_custom(quintilesOtro)[3] ~ "Media",
    quintilOtro == labels_custom(quintilesOtro)[4] ~ "Alta",
    quintilOtro == labels_custom(quintilesOtro)[5] ~ "La más alta"
  )) %>%
  mutate(CompetitividadMC = case_when(
    quintilMC == labels_custom(quintilesMC)[1] ~ "La más baja",
    quintilMC == labels_custom(quintilesMC)[2] ~ "Baja",
    quintilMC == labels_custom(quintilesMC)[3] ~ "Media",
    quintilMC == labels_custom(quintilesMC)[4] ~ "Alta",
    quintilMC == labels_custom(quintilesMC)[5] ~ "La más alta"
  ))%>%
  mutate(CompetitividadSHH= case_when(
    quintilSHH == labels_custom(quintilesSHH)[1] ~ "La más baja",
    quintilSHH == labels_custom(quintilesSHH)[2] ~ "Baja",
    quintilSHH == labels_custom(quintilesSHH)[3] ~ "Media",
    quintilSHH == labels_custom(quintilesSHH)[4] ~ "Alta",
    quintilSHH == labels_custom(quintilesSHH)[5] ~ "La más alta"
  )) %>%
  mutate(CompetitividadPT = case_when(
    quintilPT == labels_custom(quintilesPT)[1] ~ "La más baja",
    quintilPT == labels_custom(quintilesPT)[2] ~ "Baja",
    quintilPT == labels_custom(quintilesPT)[3] ~ "Media",
    quintilPT == labels_custom(quintilesPT)[4] ~ "Alta",
    quintilPT == labels_custom(quintilesPT)[5] ~ "La más alta"
 )) %>%
  mutate(CompetitividadPVEM = case_when(
    quintilPVEM == labels_custom(quintilesPVEM)[1] ~ "La más baja",
    quintilPVEM == labels_custom(quintilesPVEM)[2] ~ "Baja",
    quintilPVEM == labels_custom(quintilesPVEM)[3] ~ "Media",
    quintilPVEM == labels_custom(quintilesPVEM)[4] ~ "Alta",
    quintilPVEM == labels_custom(quintilesPVEM)[5] ~ "La más alta"
  ))

# Transformar sistema de coordenadas
mapa_DL17 <- st_transform(mapa_DL17, crs = 4326)
Secciones_MICHOACAN_DL17<- st_transform(Secciones_MICHOACAN_DL17 , crs = 4326)
Manzanas_MICHOACAN_DL17 <- st_transform(Manzanas_MICHOACAN_DL17, crs = 4326)
DistritoF_MICHOACAN_DL17 <- st_transform(DistritoF_MICHOACAN_DL17, crs = 4326)
Municipios_MICHOACAN_DL17 <- st_transform(Municipios_MICHOACAN_DL17, crs = 4326)
DistritoL_MICHOACAN_DL17 <- st_transform(DistritoL_MICHOACAN_DL17, crs = 4326)
Colonias_MICHOACAN_DL17 <- st_transform(Colonias_MICHOACAN_DL17, crs = 4326)

# Renombrar los resultados 
mapa_DL17$GanadorC_21 <- recode(mapa_DL17$GanadorC_21,
                                  "EMPATE" = "Empate",
                                  "FAM" = "Fuerza & Corazón por México",
                                  "SHH" = "Sigamos Haciendo Historia",
                                  "MC" = "Movimiento Ciudadano")

